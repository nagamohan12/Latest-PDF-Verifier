 <tr>
  <td><%= user.username %></td>
      <td><%= link_to user.email, admin_user_path(user) %>
  </td>
      <td><%= user.created_at %></td>
  <td>
    <%= link_to "Edit", edit_admin_user_path(user), :class => "btn btn-primary btn-xs"%>
    <%= link_to "Destroy", admin_user_path(user), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}", :class => "btn btn-primary btn-xs" %>
  </td>
</tr>